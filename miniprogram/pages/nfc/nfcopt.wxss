/* pages/nfc/nfcopt.wxss */
.search-bluetooth,.stop-search{
  width: 300rpx;
  height: 100rpx;
}
.view-header{
  width: 100%;
  height: 120rpx;
  top:0;
  position: fixed;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-self: center;
  flex-grow: 1;
  box-shadow: 0px 5px 8px #cccccc;
  padding-top: 10rpx;
  background-color: #fff;
}
.content{
  width: 100%;
  margin-top: 150rpx;
}
.text-content{
  width: 100%;
  word-break:break-all;
}